<%- include('partials/header') %>

<div class="w-full min-h-screen bg-zinc-800 text-white">
    <div class="cards flex flex-wrap gap-10 px-10 py-5">
        <% post.forEach(function(ele){ %>
            <div class="w-32 card">
                <div class="card w-full h  rounded-md  overflow-hidden">
                    <img class="w-full h-full object-cover bg-zinc-500"
                    src = "/images/uploads/<%= ele.image 
                    %>"></img>
                </div>
                <h4 class="mt-3 text-sm px-2"><%=ele.title %></h4>
                <h5 class="mt-1 text-md opacity-30 px-3 text-white">@<%=ele.user.username %></h5>
                
            </div>
       <%}) %>
        
      
    </div>

</div>


<%- include('partials/footer') %>
<%- include('partials/header') %>

<div class="w-full min-h-screen bg-zinc-800 text-white">
    <div class="cards flex flex-wrap gap-10 px-10 py-5">
        <% user.post.forEach(function(ele){ %>
            <div class="w-32 card">
                <div class="card w-full h-40  rounded-md bg-sky-500 overflow-hidden">
                    <img src = "/images/uploads/<%= ele.image 
                    %>"></img>
                </div>
                <h4 class="mt-3 text-sm px-2"><%=ele.    title %></h4>
            </div>
       <%}) %>
        
      
    </div>

</div>


<%- include('partials/footer') %>